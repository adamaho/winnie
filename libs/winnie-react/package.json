{
  "name": "winnie-react",
  "version": "0.0.1",
  "description": "A primitive set of react unstyled components",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./button": {
      "import": "./src/components/button.tsx",
      "types": "./src/components/button.tsx"
    },
    "./command": {
      "import": "./src/components/command.tsx",
      "types": "./src/components/command.tsx"
    },
    "./flex": {
      "import": "./src/components/flex.tsx",
      "types": "./src/components/flex.tsx"
    },
    "./heading": {
      "import": "./src/components/heading.tsx",
      "types": "./src/components/heading.tsx"
    },
    "./multiselect-list": {
      "import": "./src/components/multiselect-list.tsx",
      "types": "./src/components/multiselect-list.tsx"
    },
    "./popover": {
      "import": "./src/components/popover.tsx",
      "types": "./src/components/popover.tsx"
    },
    "./text": {
      "import": "./src/components/text.tsx",
      "types": "./src/components/text.tsx"
    }
  },
  "scripts": {
    "dev": "tsc --noEmit --watch",
    "clean": "rm -rf node_modules",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@types/react": "18.2.22",
    "eslint-config-winnie": "workspace:*",
    "react": "18.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "18.2.0"
  },
  "dependencies": {
    "@radix-ui/react-context": "^1.0.1",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-use-controllable-state": "^1.0.1",
    "cmdk": "^0.2.0"
  }
}
